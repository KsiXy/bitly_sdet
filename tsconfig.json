{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["es6", "dom"],
    "types": ["cypress", "node"],
    "allowJs": true,
    "skipDefaultLibCheck": true, //skip checks for the libraries that you can't control and example node_modules
    "strict": true
  },
  "include": ["**/*.ts"],
  "exclude": ["cypress.config.ts", "cypress", "node_modules"]
}
